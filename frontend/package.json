    "license": "MIT",
    "scripts": {
        "build": "next build",
        "postbuild": "next-sitemap",
        "dev": "NODE_ENV=development node server.js",
        "start": "NODE_ENV=production node server.js",
        "start:build": "yarn run build && NODE_ENV=production node server.js",
        "eslint": "eslint --config ./.eslintrc --fix ./",
        "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,json}\" --config ./.prettierrc",
        "lint": "yarn run prettier && yarn run eslint",
        "type-check": "tsc --pretty --noEmit",
        "husky": "cd .. && husky install frontend/.husky",
        "test": "NODE_ENV=development jest -u",
        "test-all": "yarn lint && yarn type-check && jest"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run test-all"
        }
    },
